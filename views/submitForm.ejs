<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
  </head>
  <body>
    <link rel='stylesheet' type='text/css' href='/stylesheet/submitForm_style.css' />
 
  <div class="menu">
    <div class="menu_tab">
      <a class="active" href="#home">Home</a>
      <a href="#news">News</a>
      <a href="#contact">Contact</a>
      <a href="#about">About</a>
    </div>
    <div class="menu_version">
      <a1>Ver 1.0</a1>
    </div>
  </div>

  <div class="mainpage">
    <div class="title_container">
      <h1><%= title %></h1>
    </div>
    <div class="table_container">
      <div class="table_persons">
        <table id="table_persons" border="1px solid black">
          <thead>
            <tr>
                <th>Person ID</th>
                <th>LastName</th>
                <th>FirstName</th>
                <th>Address</th>
                <th>City</th>
                <th>Register</th>
            </tr>
          </thead>

          <tbody id = "tbody_table_persons">
          </tbody>
        </table>
      </div>
    </div>

    <div class="button_container">
      <input id="input_button" type="button" value="Return" onclick="returnSubmit()"/>
    </div>
  </div>
  
  <script>
    function returnSubmit(){
      window.location.href = "/";
    }
  </script>

  </body>
</html>

