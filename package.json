{
  "name": "ex-gen-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "pkg ./bin/www",
    "build:win": "pkg ./bin/www --targets node16-win-x64 --output myapp.exe"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql2": "^3.11.3",
    "socket.io": "^4.8.0",
    "tedious": "^18.6.1"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "bin": "index.js",
  "pkg": {
    "assets": [
      "views/**/*",
      "public/**/*"
    ],
    "outputPath": "dist"
  }
}
